<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:BingWallAvalonia"
             x:Class="BingWallAvalonia.App">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Assets/ResourceSvgDictionary.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources> 
	<Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Light"/> 
		<StyleInclude Source="/Assets/Themes/LightTheme.xaml" />

 
		<StyleInclude Source="/Assets/Dialogs.xaml" />
    </Application.Styles>
</Application>